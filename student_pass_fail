sub1 = int(input("Enter the marks of maths:"))
sub2 = int(input("Enter the marks of science:"))
sub3 = int(input("Enter the marks of sst:"))

if (sub1 < 33 or sub2 < 33 or sub3 < 33):
  print("You have failed because you get less than 33% marks")
elif (sub1 + sub2 + sub3) / 3 < 40:
  print("You have failed because the average marks are less than 40")
else:
  print("Congrats! You have passed")
